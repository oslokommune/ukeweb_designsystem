{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["plugin:vue/vue3-essential", "airbnb-base", "prettier"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["prettier", "vue"],
  "rules": {
    "prettier/prettier": ["error"],
    "no-param-reassign": [2, { "props": false }]
  }
}

{
  "name": "styleguide",
  "version": "0.99.65",
  "description": "Oslo Styleguide",
  "main": "index.js",
  "scripts": {
    "build:icons": "npm run build:icons:minimize && run-p build:icons:svg build:icons:font",
    "build:icons:minimize": "svgo src/assets/icons/*.svg",
    "build:icons:svg": "svgstore src/assets/icons/*.svg -o src/assets/images/OsloIcons.svg",
    "build:icons:font": "npm run build:icons:webfont",
    "build:icons:webfont": "webfont \"src/assets/icons/**/*.svg\" --font-name 'OsloIcons' --font-height 512 --normalize --dest \"src/assets/fonts/\" --template \"src/utils/_icons.scss.njk\" --dest-template \"src/system/\"",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com:oslokommune/styleguide.git"
  },
  "author": "Utviklings- og kompetanseetaten - Oslo kommune <devnull@uke.oslo.kommune.no>",
  "license": "MIT",
  "homepage": "https://github.com/oslokommune/styleguide#readme",
  "devDependencies": {
    "npm-run-all": "4.1.5",
    "svgo": "2.8.0",
    "svgstore-cli": "2.0.1",
    "webfont": "11.2.26"
  },
  "dependencies": {
    "@nrk/core-datepicker": "^3.1.0",
    "maplibre-gl": "^1.15.2",
    "vue": "^2.6.14"
  },
  "prettier": {
    "printWidth": 400
  }
}
